{
  "expo": {
    "name": "grafica-pelotense",
    "slug": "grafica-pelotense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/grafica-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tuchgabriel.graficapelotense"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/grafica-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a0f6389-8750-4424-b3aa-7c22b3adb291"
      }
    }
  }
}
